{
  "name": "slick-react-ui-components",
  "version": "1.0.0",
  "description": "react responsive components with form validations and responsiveness",
  "author": "RaghavendraNK And Karthik",
  "license": "MIT",
  "repository": "RaghavendraNK/slick-react-ui-components",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --css-modules '[local]' --compress --tsconfig tsconfig.json --format modern,cjs",
    "start": "microbundle-crl watch --css-modules '[local]'  --compress --tsconfig tsconfig.json --format modern,cjs",
    "prepare": "run-s build",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "format": "prettier --write \"{,!(node_modules)/**/}*.js\"",
    "scss-lint": "sass-lint -v",
    "scss-lint:fix": "sass-lint-auto-fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "classnames": "^2.3.1",
    "formik": "^2.2.6",
    "react-number-format": "^4.5.5",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.2.9",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^25.1.4",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.7",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.2",
    "css-loader": "^5.2.4",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.2.0",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.11",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^4.2.0",
    "postcss-normalize": "^9.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-safe-parser": "^5.0.2",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "resolve-url-loader": "^3.1.2",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "typescript": "^3.7.5",
    "url-loader": "^4.1.1"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "classnames": "^2.3.1",
    "formik": "^2.2.6",
    "react-number-format": "^4.5.5",
    "yup": "^0.32.9"
  },
  "sasslintConfig": ".sasslintrc.json"
}
