{
  "arrowParens": "always",
  "jsxSingleQuote": true,
  "printWidth": 100,
  "singleQuote": true,
  "trailingComma": "es5",
  "disableLanguages": [
    "vue"
  ],
  "tabWidth": 4,
  "overrides": [
    {
      "files": [
        ".*.json",
        "*.json",
        ".babelrc"
      ],
      "options": {
        "tabWidth": 2
      }
    },
    {
      "files": [
        "*.yaml",
        "*.yml"
      ],
      "options": {
        "tabWidth": 2
      }
    }
  ]
}